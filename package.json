    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-vue": "^10.1.0",
    "globals": "^16.1.0",
    "husky": "^9.1.7",
    "lavy": "^1.1.0",
    "lint-staged": "^16.0.0",
    "mockjs": "^1.1.0",
    "prettier": "^3.5.3",
    "rollup-plugin-visualizer": "^6.0.3",
    "sass": "^1.86.3",
    "terser": "^5.43.1",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.32.0",
    "unplugin-auto-import": "^19.1.2",
